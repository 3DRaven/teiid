<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="urn:jboss:module:1.3" name="com.couchbase.client">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>
    
    <resources>
		<resource-root path="java-client-${version.com.couchbase.client}.jar"/>
		<resource-root path="core-io-${version.com.couchbase.core.io}.jar"/>
		<resource-root path="rxjava-${version.io.reactivex}.jar"/>
        <resource-root path="opentracing-api-${version.io.opentracing-api}.jar "/>
    </resources>

    <dependencies>
        <module name="javax.api"/>
        <module name="sun.jdk"/>
    </dependencies>
</module>
